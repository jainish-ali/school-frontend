{
  "name": "itms",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4200 --open",
    "build": "ng build",
    "watch": "ng build --watch --configuration dev",
    "test": "ng test",
    "build-dev": "ng build --configuration=dev",
    "build-staging": "ng build --configuration=staging",
    "build-production": "ng build --configuration=production",
    "lint": "ng lint",
    "lint:prettier": "prettier \"src/**/*.ts\"  --check",
    "format:prettier": "prettier \"src/**/*.ts\" --write",
    "e2e": "ng e2e",
    "postinstall": "ngcc",
    "serve:ssr": "node dist/mohawk-xchange/server/main.js",
    "prerender": "ng run mohawk-xchange:prerender",
    "prepare": "husky install",
    "gzipcompress": "gulp compress"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.1",
    "@angular/cdk": "^14.2.0",
    "@angular/common": "^15.2.10",
    "@angular/compiler": "^15.2.1",
    "@angular/core": "^15.2.10",
    "@angular/forms": "^15.2.1",
    "@angular/material": "^15.2.4",
    "@angular/platform-browser": "^15.2.1",
    "@angular/platform-browser-dynamic": "^15.2.1",
    "@angular/platform-server": "^15.2.1",
    "@angular/router": "^15.2.1",
    "@angular/service-worker": "^15.2.1",
    "@fortawesome/angular-fontawesome": "^0.11.1",
    "@fortawesome/fontawesome-svg-core": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@ng-select/ng-select": "^9.0.2",
    "@nguniversal/express-engine": "^14.2.2",
    "@solidexpert/ng-sidebar": "^14.0.1",
    "bootstrap": "^5.2.0",
    "bootstrap-icons": "^1.9.1",
    "html2canvas": "^1.4.1",
    "husky": "^8.0.2",
    "idb-keyval": "^6.2.0",
    "jspdf": "^2.5.1",
    "ngx-bootstrap": "^9.0.0",
    "ngx-bootstrap-navbar": "^14.0.0",
    "ngx-easy-table": "^15.3.0",
    "ngx-owl-carousel-o": "^14.0.1",
    "ngx-pagination": "^6.0.2",
    "ngx-spinner": "^14.0.0",
    "ngx-toastr": "^15.2.2",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.1",
    "@angular/cli": "~15.2.1",
    "@angular/compiler-cli": "^15.2.1",
    "@types/jasmine": "~4.0.0",
    "gulp": "^4.0.2",
    "gulp-gzip": "^1.4.2",
    "gulp-minify": "^3.1.0",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(ts)": [
      "eslint"
    ],
    "*.+(ts|json|css|md)": [
      "prettier --write"
    ]
  }
}
